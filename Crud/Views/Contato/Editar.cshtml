@{
    ViewData["Title"] = "Editar contato";
}

<div class="text-center">

    <h1> Editar contato</h1>
</div>

<form>


    <div class="form-group mb-2">
        <label for="nome">Nome</label>
        <input type="text" class="form-control" placeholder="Digite o nome do contato">

    </div>

    <div class="form-group mb-2">
        <label for="email">Email</label>
        <input type="text" class="form-control" placeholder="Digite o email do contato">
    </div>

    <div class="form-group mb-2">
        <label for="telefone">Telefone</label>
        <input type="text" class="form-control" placeholder="Digite o telefone do contato">
    </div>
    <br />
    <button type="submit" class="btn btn-primary">Alterar</button>
    <a class="btn btn-secondary" asp-controller="Contato" asp-action="Index" role="button">Voltar</a>


</form>
